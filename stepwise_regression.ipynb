    "m = len(df1['y'])\n",
    "p = len(features)\n",
    "hat_sigma_squared = (1/(m - p -1)) * min(model['sse'])\n",
    "model['C_p'] = (1/m) * (model['sse'] + 2 * p * hat_sigma_squared )\n",
    "best_model = model.loc[model['C_p'].idxmin()]\n",
    "best_model['model'].summary()"
   ]
